#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i;
    int upper=0,lower=0,num=0;
    char ch[100];

    printf("Input:\n");
    gets(ch);

    for(i=0; ch[i]!=0; i++){

if(ch[i]>='A' && ch[i]<='Z'){
    upper++;
}
else if(ch[i]>='a' && ch[i]<='z'){
    lower++;
}
else if(str[i]>='1' && str[i]<='9'){
     num++;
    }
}
printf("lowercase letters: %d",lower);
printf("\nuppercase letters: %d",upper);
printf("\nNumbers: %d",num);
getch();
    return 0;
}
