#include <stdio.h>
#include <string.h>
 

int main()
{

    char str[] = { " " };

    int l = 0;

    int h = strlen(str) - 1;

    while (h > l) {

        if (str[l++] != str[h--]) {

            printf(" not a palindrome\n", str);

            return 0;

        }

    }
 

    printf(" palindrome\n", str);
 

    return 0;
}
